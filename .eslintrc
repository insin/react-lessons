{
  "extends": ["standard", "standard-react"],
  "rules": {
    "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
    "react/jsx-quotes": 0,
    "react/prop-types": 0,
    "react/wrap-multilines": 0,
    "space-before-function-paren": [2, {
      "anonymous": "always",
      "named": "never"
    }],
  },
  "parser": "babel-eslint"
}
